# interpreter for shell commands
set shell powershell
set hidden false
set hidden!

map m $micro $Env:f

map N $nvim $Env:f

map n #reiniciamos n
map nf $New-Item -Path 'nuevo.txt' -ItemType File
map nd $mkdir new_directory


# change the shell used in default shell keybinding
map w $powershell

map D :delete

map F $fzf

# NAVEGACIÃ“N
map gd cd "~\Downloads"
map ge cd "~\Desktop"
map gA cd "~\AppData"

# COPIAR Y PEGAR
map C copy
map V paste

map E $start .
