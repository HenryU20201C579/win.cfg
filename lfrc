# interpreter for shell commands
set shell powershell
set hidden true

# change the default open command to work in powerShell
cmd open &start $Env:f

map m $micro $Env:f

map n $nvim $Env:f

# change the shell used in default shell keybinding
map w $powershell

map D :delete

map F $fzf

# NAVEGACIÃ“N
map gd cd "~\Downloads"
map ge cd "~\Desktop"
map gA cd "~\AppData"

# COPIAR Y PEGAR
map C copy
map V paste

map E $start .
